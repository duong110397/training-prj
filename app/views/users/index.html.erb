<h1> All Users </h1>
<% if current_user.admin? %>
  <%= link_to "add user", new_user_path, :class => "btn btn-primary" %>
<% end %>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Email</th>
      <th scope="col">Name</th>
      <th scope="col">Birthday</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <%= render user %>
    <% end %>
  </tbody>
</table>
<%= link_to t('.link_export'), export_users_path(format: :csv), :class => "btn btn-primary" %>
